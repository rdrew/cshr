{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region <section>.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{%
set classes = [
'region',
'region--' ~ region|clean_class,
]
%}
{% if content %}
  <section{{ attributes.addClass(classes) }}>
    <a href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home" class="upei-logo">
      <img src="/themes/custom/cshr/dist/assets/img/logo.svg" alt="{{ 'Home'|t }}" />
    </a>
    {{ content }}
  </section>
{% endif %}
